      "source": [
        "sample_size = 30\n",
        "sample = sps.norm.rvs(size=sample_size)  # выборка\n",
        "\n",
        "asymptotic_ci = [sample.mean()-(sps.norm(loc=0, scale=1).ppf(alpha))*2/np.sqrt(sample_size), sample.mean()+(sps.norm(loc=0, scale=1).ppf(alpha))*2/np.sqrt(sample_size)]  # асимптотические ДИ\n",
        "\n",
        "precise_ci = [sample.mean()-(sps.t(loc=0, scale=1, df = sample_size-1).ppf(alpha))*2/np.sqrt(sample_size-1), sample.mean()+(sps.t(loc=0, scale=1, df = sample_size-1).ppf(alpha))*2/np.sqrt(sample_size-1)] # точные ДИ\n",
        "\n",
        "data = {\n",
        "    'асимптотический интервал': asymptotic_ci,\n",
        "    'точный интервал': precise_ci\n",
        "}\n",
        "\n",
        "df = pd.DataFrame(data)\n",
        "\n",
        "# выводим таблицу\n",
        "print(df)"
      ]
